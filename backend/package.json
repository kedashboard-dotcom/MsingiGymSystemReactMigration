{
  "name": "msingi-gym-backend",
  "version": "2.0.0",
  "description": "Msingi Gym System Backend API with React Integration",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest",
    "test:mpesa": "node test-mpesa.js",
    "test:database": "node test-database.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
